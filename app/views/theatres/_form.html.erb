<%= form_for @theatre do |f| %>

<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :address1 %>
<%= f.text_field :address1 %><br>

<%= f.label :address2 %>
<%= f.text_field :address2 %><br>

<%= f.label :city %>
<%= f.text_field :city %><br>

<%= f.label :postcode %>
<%= f.text_field :postcode %><br>

<%= f.file_field :picture %>

<%= f.submit %>

<% end %>
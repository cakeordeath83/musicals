<%= form_for @theatre do |f| %>

<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :address %>
<%= f.text_field :address %><br>

<%= f.label :address2 %>
<%= f.text_field :address2 %><br>

<%= f.label :city %>
<%= f.text_field :city %><br>

<%= f.label :postcode %>
<%= f.text_field :postcode %><br>

<%= f.label :longitude %>
<%= f.text_field :longitude %><br>

<%= f.label :latitude %>
<%= f.text_field :latitude %><br>


<%= f.submit %>

<% end %>